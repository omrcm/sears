{
    "name": "antimattr/sears",
    "type": "library",
    "description": "Sears API Client",
    "keywords": ["sears", "marketplace", "api"],
    "homepage": "http://github.com/antimattr/sears",
    "license": "MIT",
    "authors": [
        {"name": "Matthew Fitzgerald", "email": "matthewfitz@gmail.com"},
        {"name": "Cara Warner", "email": "warnercara@gmail.com"}
    ],
    "require": {
        "php": ">=5.3.2",
        "doctrine/collections": "1.*",        
        "kriswallsmith/buzz": "~0.10",
        "psr/log": "1.0.*@dev"
    },
    "require-dev": {
        "antimattr/test-case": "~1.0@stable",
        "phpunit/phpunit": "~4.0",
        "fabpot/php-cs-fixer": "0.5.*@dev"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "AntiMattr\\Sears\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "AntiMattr\\TestCase\\": "vendor/antimattr/test-case/src",
            "AntiMattr\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "archive": {
        "exclude": ["bin", "tests", "*phpunit.xml"]
    }
}
